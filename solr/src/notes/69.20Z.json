  "niveau" : "sousClasse",
  "parents" : [ "M", "69", "69.2", "69.20" ],
  "comprend" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>l'enregistrement d'opérations commerciales pour les entreprises ou autres</p>\n            </li>\n            <li>\n                <p>l'établissement ou la vérification de comptes financiers</p>\n            </li>\n            <li>\n                <p>l'examen des comptes et la certification de leur exactitude</p>\n            </li>\n            <li>\n                <p>l'établissement de déclarations fiscales pour les particuliers et les entreprises</p>\n            </li>\n            <li>\n                <p>les activités de conseil et de représentation (autre que la représentation juridique), pour le compte de clients, devant l'administration fiscale</p>\n            </li>\n        </ul>\n    </div:div>\n",
  "noteGenerale" : null,
  "comprendAussi" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>l'activité des centres de gestion agréés</p>\n            </li>\n        </ul>\n    </div:div>\n",
  "neComprendPas" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>les activités de traitement et de tabulation des données (cf. \n                    <a href=\"http://dvrmessnclas01.ad.insee.intra:8080/datalift/codes/nafr2/sousClasse/63.11Z\">63.11Z</a>)</p>\n            </li>\n            <li>\n                <p>le conseil de gestion en matière de systèmes comptables, les procédures de contrôle budgétaire (cf. \n                    <a href=\"http://dvrmessnclas01.ad.insee.intra:8080/datalift/codes/nafr2/sousClasse/70.22Z\">70.22Z</a>)</p>\n            </li>\n            <li>\n                <p>le recouvrement de factures (cf. \n                    <a href=\"http://dvrmessnclas01.ad.insee.intra:8080/datalift/codes/nafr2/sousClasse/82.91Z\">82.91Z</a>)</p>\n            </li>\n        </ul>\n    </div:div>\n"
