{
  "code" : "42.21Z",
  "libelle" : "Construction de réseaux pour fluides",
  "niveau" : "sousClasse",
  "parents" : [ "F", "42", "42.2", "42.21" ],
  "comprend" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>la construction d'ouvrages de génie civil pour :</p>\n                <ul class=\"list1\">\n                    <li>\n                        <p>conduites couvrant de longues distances et urbaines</p>\n                    </li>\n                    <li>\n                        <p>conduites et canalisations d'eau</p>\n                    </li>\n                    <li>\n                        <p>systèmes d'irrigation (canaux)</p>\n                    </li>\n                    <li>\n                        <p>réservoirs</p>\n                    </li>\n                </ul>\n            </li>\n            <li>\n                <p>la construction de :</p>\n                <ul class=\"list1\">\n                    <li>\n                        <p>réseaux d'assainissement, y compris leur réparation</p>\n                    </li>\n                    <li>\n                        <p>stations d'épuration</p>\n                    </li>\n                    <li>\n                        <p>stations de pompage</p>\n                    </li>\n                </ul>\n            </li>\n        </ul>\n    </div:div>\n",
  "noteGenerale" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <p>Cette sous-classe comprend la construction de lignes de distribution pour le transport de fluides et des bâtiments et structures faisant partie intégrante de ces systèmes.</p>\n    </div:div>\n",
  "comprendAussi" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>le forage de puits d'eau</p>\n            </li>\n            <li>\n                <p>l'installation de fosses septiques</p>\n            </li>\n        </ul>\n    </div:div>\n",
  "neComprendPas" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>les services de maîtrise d'œuvre sur ouvrages de génie civil (cf. \n                    <a href=\"http://dvrmessnclas01.ad.insee.intra:8080/datalift/codes/nafr2/groupe/71.1\">71.1</a>)</p>\n            </li>\n            <li>\n                <p>l'installation de systèmes d'arrosage automatique des pelouses (cf. \n                    <a href=\"http://dvrmessnclas01.ad.insee.intra:8080/datalift/codes/nafr2/sousClasse/43.22A\">43.22A</a>)</p>\n            </li>\n        </ul>\n    </div:div>\n"
}
