  "parents" : [ "M", "74", "74.9", "74.90" ],
  "comprend" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>les études, métrés, vérifications tous corps d'état relatifs à des projets de construction</p>\n            </li>\n            <li>\n                <p>la maîtrise des coûts des projets de construction :</p>\n                <ul class=\"list1\">\n                    <li>\n                        <p>assistance à la mise au point de projet de construction</p>\n                    </li>\n                    <li>\n                        <p>description technique des ouvrages</p>\n                    </li>\n                    <li>\n                        <p>établissement et contrôle des estimations prévisionnelles</p>\n                    </li>\n                    <li>\n                        <p>analyse des offres des entreprises</p>\n                    </li>\n                    <li>\n                        <p>suivi administratif des marchés</p>\n                    </li>\n                    <li>\n                        <p>arrêté des comptes de chantiers</p>\n                    </li>\n                </ul>\n            </li>\n            <li>\n                <p>l'ordonnancement, la planification et la coordination des chantiers</p>\n            </li>\n        </ul>\n    </div:div>\n",
  "noteGenerale" : null,
  "comprendAussi" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>l'assistance à maîtrise d'ouvrage relative à des projets de construction</p>\n            </li>\n        </ul>\n    </div:div>\n"
