{
  "code" : "88.10C",
  "libelle" : "Aide par le travail",
  "niveau" : "sousClasse",
  "parents" : [ "Q", "88", "88.1", "88.10" ],
  "comprend" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>les activités de réadaptation professionnelle et réinsertion des handicapés, à condition que ces activités ne comportent qu'un élément pédagogique limité :</p>\n                <ul class=\"list1\">\n                    <li>\n                        <p>les activités des établissements et services d'aide par le travail, des centres de rééducation professionnelle, etc.</p>\n                    </li>\n                </ul>\n            </li>\n        </ul>\n    </div:div>\n",
  "noteGenerale" : null,
  "neComprendPas" : "\n    <div:div xmlns:div=\"http://www.w3.org/1999/xhtml\" xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:owl=\"http://www.w3.org/2002/07/owl#\" xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\" xmlns:fn=\"http://www.w3.org/2005/xpath-functions#\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema#\" xmlns:rdfs=\"http://www.w3.org/2000/01/rdf-schema#\" xmlns:sesame=\"http://www.openrdf.org/schema/sesame#\">\n        <ul class=\"list1\">\n            <li>\n                <p>les activités des entreprises adaptées, ni celles des centres de distribution de travail à domicile (CDTD) ; ce type d'unités doit être classé en fonction de l'activité réellement exercée.</p>\n            </li>\n        </ul>\n    </div:div>\n"
}
